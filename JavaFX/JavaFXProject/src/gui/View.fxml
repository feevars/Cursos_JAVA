<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <Button fx:id="btDeposito" layoutX="315.0" layoutY="260.0" mnemonicParsing="false" onAction="#onBtSumAction" text="Depositar">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font></Button>
      <Label layoutX="14.0" layoutY="23.0" text="Conta bancária - Depósitos">
         <font>
            <Font name="Arial Black" size="14.0" />
         </font></Label>
      <TextField fx:id="txtDeposito" layoutX="14.0" layoutY="70.0" prefHeight="25.0" prefWidth="126.0" promptText="Valor de depósito" />
      <Text layoutX="14.0" layoutY="280.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total: R$">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Label fx:id="labelSaldo" layoutX="99.0" layoutY="258.0" prefHeight="17.0" prefWidth="38.0">
         <font>
            <Font size="20.0" />
         </font></Label>
   </children>
</AnchorPane>
